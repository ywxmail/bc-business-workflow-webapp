<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carManEntry.OperationSecurityInterviewForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carManEntry/OperationSecurityInterview.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
	<div style="width:640px;">
		<table cellspacing="2" cellpadding="0" style="width:640px;min-width: 600px;">
			<tr class="widthMarker">
				<td style="width: 4em;"></td>
				<td></td>
			</tr>
			<tr>
				<td class="topLabel" >评价：</td>
				<td class="value" >
					<textarea  name="osiValuation" class="ui-widget-content noresize" data-validate="required"
				 		rows="3" placeholder="这里填写对司机的评价"></textarea>
				</td>
			</tr>
		</table>
	</div>
	<#else>
	<div style="width:640px;">
		<div class="text">
			评价：${osiValuation!}。
		</div>
	</div>
	</#if>
</form>