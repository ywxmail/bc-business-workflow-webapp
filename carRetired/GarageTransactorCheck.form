<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.carRetired.GarageTransactorCheckForm"
	data-js='bc-business-workflow/carRetired/GarageTransactorCheck.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
		<table cellspacing="4" cellpadding="0" style="width:100%;min-width: 400px;">
			<tr class="header row">
				<td style="width: 10em;text-align: center;">设备名</td>
				<td style="width: 5em;text-align: center;">*设备情况</td>
				<td style="width: 50%;text-align: center;">备注</td>
				<td style="min-width:0.01em">&nbsp;</td>
			</tr>
			<tr>
				<td class="value" >车牌号码（前、后）</td>
				<td class="value">
					<select name="gtcPlate" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcPlateDesc" data-scope="global" class="ui-widget-content"/></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >车钥匙</td>
				<td class="value" style="padding-left:4px;">
					<input type="text" data-scope="global" name="gtcCarKeyCount" class="ui-widget-content" style="width:2em;" data-validate="{'required':true,'type':'number'}"/>条
				</td>
				<td class="value" ><input type="text" name="gtcCarKeyDesc" data-scope="global" class="ui-widget-content"  /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >顶灯装置</td>
				<td class="value">
					<select name="gtcDDZZ" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" data-scope="global" name="gtcDDZZDesc" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >计价器</td>
				<td class="value">
					<select name="gtcJJQ" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcJJQDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >远距离密标</td>
				<td class="value">
					<select name="gtcYJLMB" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcYJLMBDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >近距离密标</td>
				<td class="value">
					<select name="gtcJJLMB" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcJJLMBDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >城市电视</td>
				<td class="value">
					<select name="gtcCityTv" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcCityTvDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >触动传媒</td>
				<td class="value">
					<select name="gtcTouchMedia" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcTouchMediaDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >防盗网</td>
				<td class="value">
					<select name="gtcFDW" data-scope="global"  class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcFDWDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >LPG设备</td>
				<td class="value">
					<select name="gtcLPG" data-scope="global" class=" ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcLPGDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
			<tr>
				<td class="value" >备胎</td>
				<td class="value">
					<select name="gtcBT" data-scope="global" class="ui-widget-content" style="text-align:left;border:none;"
					           data-validate="required">
							<option value="已回收">已回收</option>
							<option value="未回收">未回收</option>
							<option value="已遗失">已遗失</option>
							<option value="无安装">无安装</option>
					</select>
				</td>
				<td class="value" ><input type="text" name="gtcBTDesc" data-scope="global" class="ui-widget-content" /></td>
				<td></td>
			</tr>
		</table>
		<table cellspacing="4" cellpadding="0" style="width:100%;min-width: 400px;">
			<tr class="widthMarker">
				<td style="width:10em;">&nbsp;</td>
				<td style="width:15em;">&nbsp;</td>
				<td style="width:13em;">&nbsp;</td>
				<td style="width:15em;">&nbsp;</td>
				<td >&nbsp;</td>
			</tr>
			<tr>
				<td class="label">发动机拓印号码：</td>
				<td class="value">
					<input type="text" name="engineRubNo" data-scope="global" class="ui-widget-content" placeholder="交车性质为”其他“时填写"/>
				</td>
				<td class="label">车身拓印号码：</td>
				<td class="value">
					<input type="text" name="carBodyRubNo" data-scope="global" class="ui-widget-content" placeholder="交车性质为”其他“时填写"/>
				</td>
				<td></td>
			</tr>
			<tr>
				<td class="label">车辆回收估价：</td>
				<td class="value">
					<input type="text" name="gtcValuationMoney" data-scope="global" data-validate="number" class="ui-widget-content" style="width:8em;"
					 placeholder="需要估价时填写"/>(元)
				</td>
				<td></td>
				<td></td>
				<td></td>
			</tr>
			<tr>
				<td class="topLabel">车质车况说明：</td>
				<td class="value" colspan="3">
					<textarea name="gtcDesc"  data-scope="global" rows="3"  class="ui-widget-content noresize" ></textarea>
				</td>
				<td></td>
			</tr>
			<tr>
				<td class="label">羊城通驻点员名称：</td>
				<td class="value">
					<input type="text"  data-scope="global" name="gtcYCTStationName" class="ui-widget-content" />
				</td>
				<td class="label">羊城通设备是否齐全：</td>
				<td class="value">
					<select name="gtcYCTisComplete" data-scope="global" data-type="boolean" class="ui-widget-content" style="text-align:left;width:8em;">
						<option ></option>
						<option value="true">是</option>
						<option value="false">否</option>
					</select>	
				</td>
				<td></td>
			</tr>
			<tr>
				<td class="label">羊城通有无欠费：</td>
				<td class="value">
					<select name="gtcYCTisDebt" data-scope="global" data-type="boolean" class="ui-widget-content" style="text-align:left;width:8em;">
						<option ></option>
						<option value="true">有</option>
						<option value="false">无</option>
					</select>
				</td>
				<td class="label">羊城通欠费金额：</td>
				<td class="value">
					<div id="gtcDebt">
						<input type="text"  data-scope="global" name="gtcYCTDebtMoney" class="ui-widget-content" 
							 data-validate="number" style="width:5em;" />(元)
					</div>
				</td>
				<td></td>
			</tr>
			<tr>
				<td class="topLabel">羊城通备注：</td>
				<td class="value" colspan="3">
					<textarea name="gtcYCTDesc"  data-scope="global" rows="3"  class="ui-widget-content noresize" ></textarea>
				</td>
				<td></td>
			</tr>
		</table>
	<#else>
		<table class="" cellspacing="4" cellpadding="0" style="width:100%;min-width: 400px;">
			<tr class="header row">
				<td style="width: 10em;text-align: center;">设备名</td>
				<td style="width: 5em;text-align: center;">设备情况</td>
				<td style="width: 50%;text-align: center;">备注</td>
				<td style="min-width:0.01em">&nbsp;</td>
			</tr>
			<tr>
				<td class="value" >车牌号码（前、后）</td>
				<td class="value" >${gtcPlate!}</td>
				<td class="value" >${gtcPlateDesc!}</td>
			</tr>
			<tr>
				<td class="value" >车钥匙</td>
				<td class="value" style="padding-left:4px;">
					${gtcCarKeyCount!}条
				</td>
				<td class="value" >${gtcCarKeyDesc!}</td>
			</tr>
			<tr>
				<td class="value" >顶灯装置</td>
				<td class="value" >${gtcDDZZ!}</td>
				<td class="value" >${gtcDDZZDesc!}</td>
			</tr>
			<tr>
				<td class="value" >计价器</td>
				<td class="value" >${gtcJJQ!}</td>
				<td class="value" >${gtcJJQDesc!}</td>
			</tr>
			<tr>
				<td class="value" >远距离密标</td>
				<td class="value" >${gtcYJLMB!}</td>
				<td class="value" >${gtcYJLMBDesc!}</td>
			</tr>
			<tr>
				<td class="value" >近距离密标</td>
				<td class="value" >${gtcJJLMB!}</td>
				<td class="value" >${gtcJJLMBDesc!}</td>
			</tr>
			<tr>
				<td class="value" >城市电视</td>
				<td class="value" >${gtcCityTv!}</td>
				<td class="value" >${gtcCityTvDesc!}</td>
			</tr>
			<tr>
				<td class="value" >触动传媒</td>
				<td class="value" >${gtcTouchMedia!}</td>
				<td class="value" >${gtcTouchMediaDesc!}</td>
			</tr>
			<tr>
				<td class="value" >防盗网</td>
				<td class="value" >${gtcFDW!}</td>
				<td class="value" >${gtcFDWDesc!}</td>
			</tr>
			<tr>
				<td class="value" >LPG设备</td>
				<td class="value" >${gtcLPG!}</td>
				<td class="value" >${gtcLPGDesc!}</td>
			</tr>
			<tr>
				<td class="value" >备胎</td>
				<td class="value" >${gtcBT!}</td>
				<td class="value" >${gtcBTDesc!}</td>
			</tr>
		</table>
		<#if engineRubNo??>
		<div style="padding-left:0.5em;">发动机拓印号码：${engineRubNo}</div>
		</#if>
		<#if carBodyRubNo??>
		<div style="padding-left:0.5em;">车身拓印号码：${carBodyRubNo}</div>
		</#if>
		<#if gtcValuationMoney??>
		<div style="padding-left:0.5em;">车辆回收估价：${gtcValuationMoney}</div>
		</#if>
		<#if gtcDesc??&&gtcDesc!="">
		<div style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">车质车况说明：</div>
			<div class="text"  style="display:inline-block;">
				${gtcDesc}
			</div>
		</div>
		</#if>
		<div style="padding-left:0.5em;">羊城通驻点员名称：${gtcYCTStationName!}</div>
		<div style="padding-left:0.5em;">
			羊城通设备是否齐全： <#if gtcYCTisComplete??><#if gtcYCTisComplete>是<#else>否</#if></#if>
		</div>
		<div style="padding-left:0.5em;">
			羊城通有无欠费：<#if gtcYCTisDebt??><#if gtcYCTisDebt>有  &nbsp; 羊城通欠费金额：<#if gtcYCTDebtMoney??>${gtcYCTDebtMoney}</#if><#else>无</#if></#if>
		</div>
		<#if gtcYCTDesc??&&gtcYCTDesc!="">
		<div style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">羊城通备注：</div>
			<div class="text" style="display:inline-block;">
				${gtcYCTDesc}
			</div>
		</div>
		</#if>
	</#if>
</form>