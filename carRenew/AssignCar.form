<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.carRenew.AssignCarForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carRenew/AssignCar.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
		<table cellspacing="4" cellpadding="0" style="width:100%;min-width: 400px;">
			<tr class="widthMarker">
				<td style="width: 15em;"></td>
				<td style="width: 18em;">&nbsp;</td>
				<td style="width: 15em;">&nbsp;</td>
				<td style="width: 18em;">&nbsp;</td>
				<td style="min-width:0.01em">&nbsp;</td>
			</tr>
			<#if !car??>
				<#assign car={"temp":1}>
			</#if> 
			<tr>
				<td class="label" >*车牌：</td>
				<td class="value">
					<div style="position:relative;">
						<input type="text"   name="plate"         value="${car.plate!}"         data-scope="global" style="width: 18em;" class="ui-widget-content" readonly="readonly" />
						<input type="hidden" name="carId"         value="${car.id!}"            data-scope="global" data-type="long" class="ui-widget-content" />
						<input type="hidden" name="plateType"     value="${car.plateType!}"     data-scope="global" />
						<input type="hidden" name="plateNo"       value="${car.plateNo!}"       data-scope="global" />
						<input type="hidden" name="unitCompany"   value="${car.unitCompany!}"   data-scope="global" />
						<input type="hidden" name="verifyUnitId"  value="${car.unitCompanyId!}" data-scope="global" data-type="long"/>
						<input type="hidden" name="motorcadeId"   value="${car.motorcadeId!}"   data-scope="global" data-type="long"/>
						<input type="hidden" name="motorcadeName" value="${car.motorcadeName!}" data-scope="global" />
						<#if car.temp??>
							<ul class="inputIcons">
								<li id="addCar" class="inputIcon ui-icon ui-icon-circle-plus" title='选择车辆'/>
							</ul>
						</#if>
					</div>
				</td>
				<td class="label" >*保险期间：</td>
				<td class="value">
					<div style="position : relative; display: inline-block">
						从&nbsp;<input type="text" name="greenslipStartDate" class="bc-date ui-widget-content" style="width: 7.25em;"   data-scope="global" data-validate='{"type":"date","required":true}'
						value="${car.greenslipStartDate!}"/>
						<ul class="inputIcons">
							<li class="selectCalendar inputIcon ui-icon ui-icon-calendar"></li>
						</ul>
					</div>					
					<div style="position : relative; display: inline-block">
						到&nbsp;<input type="text" name="greenslipEndDate" class="bc-date ui-widget-content" style="width: 7.25em;"   data-scope="global" data-validate='{"type":"date","required":true}'
						value="${car.greenslipEndDate!}"/>
						<ul class="inputIcons">
							<li class="selectCalendar inputIcon ui-icon ui-icon-calendar"></li>
						</ul>
					</div>
				</td>
			</tr>
			<tr>
				<td class="label" >*投保人：</td>
				<td class="value" >
					<input type="text" name="policyHolder" class="ui-widget-content" data-scope="global" style="width: 18em;" />
				</td>
				<td class="label" >*司机：</td>
				<td class="value" >
					<input type="text" name="driverName" readonly="readonly" class="ui-widget-content" value="${car.driverName!}" data-scope="global" style="width: 18em;" />
				</td>
			</tr>
			<tr>
				<td class="label" >*发动机号：</td>
				<td class="value" >
					<input type="text" name="engineNo" readonly="readonly" class="ui-widget-content" value="${car.engineNo!}"  data-scope="global" style="width: 18em;" />
				</td>
				<td class="label" >初登日期：</td>
				<td class="value" >
					<div class="relative">
						<input type="text" name="registerDate" class="bc-date ui-widget-content" style="width: 18em;"  data-scope="global" data-validate='{"type":"date","required":true}'
							value="${car.registerDate!}"/>
					</div>
				</td>
			</tr>
			<tr>
				<td class="label" >*厂牌型号：</td>
				<td class="value" >
					<input type="text" name="factoryType" class="ui-widget-content"  data-scope="global" data-validate="required"
						value="${car.factoryType!}" readonly="readonly" style="width: 18em;" />
				</td>
				<td class="label" >*车架号：</td>
				<td class="value" >
					<input type="text" name="vin" readonly="readonly" class="ui-widget-content" value="${car.vin!}" style="width: 18em;" />
				</td>
			</tr>
		</table>
	<#else>
		<table cellspacing="4" cellpadding="0" style="width:100%;min-width: 400px;">
			<tr class="widthMarker">
				<td style="width: 15em;"></td>
				<td style="width: 18em;">&nbsp;</td>
				<td style="width: 15em;">&nbsp;</td>
				<td style="width: 18em;">&nbsp;</td>
				<td style="min-width:0.01em">&nbsp;</td>
			</tr>
			<tr>
				<td class="label" >车牌：</td>
				<td class="value">${plate!car.plate}"</td>
				<td class="label" >保险期间：</td>
				<td class="value">
					&nbsp;从&nbsp;${greenslipStartDate!car.greenslipStartDate}
					&nbsp;到&nbsp;${greenslipEndDate!car.greenslipEndDate}
				</td>
			</tr>
			<tr>
				<td class="label" >投保人：</td>
				<td class="value" >${policyHolder!}</td>
				<td class="label" >司机：</td>
				<td class="value" >${driverName!car.driverName}</td>
			</tr>
			<tr>
				<td class="label" >*发动机号：</td>
				<td class="value" >${engineNo!car.engineNo}</td>
				<td class="label" >初登日期：</td>
				<td class="value" >${registerDate!car.registerDate}</td>
			</tr>
			<tr>
				<td class="label" >厂牌型号：</td>
				<td class="value" >${factoryType!car.factoryType}</td>
				<td class="label" >车架号：</td>
				<td class="value" >${vin!car.vin}</td>
			</tr>
		</table>
	</#if>
</form>