<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carManEntry.EmployCheckForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carManEntry/EmployCheck.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
	<div style="width:640px;">
		<div class="value" style="display:inline-block;">
			<input type="hidden"  name="ecIsEmploy" data-scope="global" data-type="boolean">
			<input type="radio" class="ignore" name="ecIsEmploy_temp" value="true" checked style="width:auto;" />
			<label>聘用</label>
			<input type="radio" class="ignore" name="ecIsEmploy_temp" value="false"  style="width:auto;" />
			<label>不聘用</label>
		</div>
	</div>
	<#else>
	<div style="width:640px;">
		<div class="text">
			<#if ecIsEmploy??><#if ecIsEmploy>聘用<#else>不聘用</#if></#if>。
		</div>
	</div>
	</#if>
</form>