<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carActive.fillInInfoForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carActive/fillInInfo.js'>
	<div class="bc-grid header" style="padding : 0px 0px;height: 100%;">
		<table cellspacing="0"  cellpadding="0" style="width: 100%;border: 1xp;">
			<tr class="widthMarker">
				<td style="width: 50px;text-align: center;">&nbsp;</td>
				<td style="width: 90px;text-align: center;">&nbsp;</td>
				<td style="width: 90px;">&nbsp;</td>
				<td style="width: 90px;">&nbsp;</td>
				<td style="width: 90px;text-align: center;">&nbsp;</td>
				<td >&nbsp;</td>
			</tr>
			<tr>
				<td class="first ui-state-default" rowspan="2">基本<br>资料</td>
				<td class="middle ui-state-default">车牌号</td>
				<td class="middle" colspan="2">
					<input type="text" name="plate_gl" class="ui-widget-content" value="${plate_gl!}" data-scope="global" style="border: 0;width: 50%;"/>
				</td>
				<td class="middle ui-state-default" >出车性质</td>
				<td class="last" >
				<!--  
					<input type="checkbox" name="isNewCar" Style="width:1em;" value="${isNewCar!}" /><label>新车</label>	
					<input type="checkbox" name="isChangeCharger" Style="width:1em;" value="${isChangeCharger!}" /><label>重发包</label>	
					<input type="checkbox" name="isChange" Style="width:1em;" value="${isChange!}" /><label>变更</label>	
				-->	
					<select name="carActiveType" class="ui-widget-content"  data-validate="required"
					        data-scope="global" style="border: none;">
					        <option></option>
							<option value="新车">新车</option>
							<option value="重发包">重发包</option>
							<option value="变更">变更</option>
					</select>
				</td>
			</tr>
			<tr>
				<td class="middle ui-state-default" >驾驶员</td>
				<td class="middle" colspan="2">
					<input type="text" name="driver1_gl" class="ui-widget-content" value="${driver1_gl!}" data-scope="global" style="width: 50px;border: 0;"/>
					<input type="text" name="driver2_gl" class="ui-widget-content" value="${driver2_gl!}" data-scope="global" style="width: 50px;border: 0;"/>
				</td>
				<td class="middle ui-state-default" >营运性质</td>
				<td class="last" >
				<!--  
					<input type="checkbox" name="isContract" Style="width:1em;" value="${isContract!}" /><label>承包</label>	
					<input type="checkbox" name="isCollaborate" Style="width:1em;" value="${isCollaborate!}" /><label>合作</label>	
					<input type="checkbox" name="isElse" Style="width:1em;" value="${isElse!}" /><label>其他</label>	
				-->
					<select name="bsType" class="ui-widget-content"  data-validate="required"
					        data-scope="global" style="border: none;">
					        <option></option>
							<option value="承包">承包</option>
							<option value="合作">合作</option>
							<option value="其它">其它</option>
					</select>
				</td>
			</tr>
			<tr>
				<td class="first ui-state-default" style="height: 26px;" rowspan="3">变更<br>填写</td>
				<td class="middle ui-state-default" style="height: 26px;">迁出人员</td>
				<td class="middle ui-state-default" style="height: 26px;">资格证号</td>
				<td class="middle ui-state-default" style="height: 26px;">迁入人员</td>
				<td class="middle ui-state-default" style="height: 26px;">资格证号</td>
				<td class="last ui-state-default" style="height: 26px;">变更性质</td>
			</tr>
			<tr>
				<td class="middle" >
					<div style="position:relative;width: 80px;">
						<input type="text" name="oldDriver1_gl" readonly="readonly" class="ui-widget-content" value="${oldDriver1_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<ul class="inputIcons">
							<li id="addOldDriver1" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
						<input type="hidden" name="oldDriverId1_gl" class="ui-widget-content" value="${oldDriverId1_gl!}" data-scope="global"/>
					</div>
				</td>
				<td class="middle" >
					<input type="text" name="certFwzg4oldDriver1_gl" class="ui-widget-content" value="${certFwzg4oldDriver1_gl!}" style="border: 0;width: 80px;"/>
				</td>
				<td class="middle" >
					<div style="position:relative;width: 80px;">
						<input type="text" name="driver1_gl" readonly="readonly" class="ui-widget-content" value="${driver1_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<ul class="inputIcons">
							<li id="addDriver1" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
					</div>
					<!--<input type="text" name="driver1_gl" class="ui-widget-content" value="${driver1_gl!}" data-scope="global" style="border: 0;width: 50%;"/>  -->
				</td>
				<td class="middle" >
					<input type="text" name="certFwzg4Driver1_gl" class="ui-widget-content" value="${certFwzg4Driver1_gl!}" data-scope="global" style="border: 0;width: 80px;"/>
				</td>
				<td class="last" >
				<!-- 
					<input type="checkbox" name="isCharger4driver1" Style="width:1em;" value="${isNewCar!}" /><label>责任人</label>	
					<input type="checkbox" name="isNoCharger4driver1" Style="width:1em;" value="${isChangeCharger!}" /><label>非责任人</label>	
				 -->
				 	<select name="charger4driver1" class="ui-widget-content"  data-validate="required"
					        data-scope="global" style="border: none;">
					        <option></option>
							<option value="责任人">责任人</option>
							<option value="非责任人">非责任人</option>
					</select>
				</td>
			</tr>
			<tr>
				<td class="middle" >
					<div style="position:relative;width: 80px;">
						<input type="text" name="oldDriver2_gl" readonly="readonly" class="ui-widget-content" value="${oldDriver2_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<ul class="inputIcons">
							<li id="addOldDriver2" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
						<input type="hidden" name="oldDriverId2_gl" class="ui-widget-content" value="${oldDriverId2_gl!}" data-scope="global"/>
					</div>
				</td>
				<td class="middle" >
					<input type="text" name="certFwzg4oldDriver2_gl" class="ui-widget-content" value="${certFwzg4oldDriver2_gl!}" style="border: 0;width: 80px;"/>
				</td>
				<td class="middle" >
					<div style="position:relative;width: 80px;">
						<input type="text" name="driver2_gl" readonly="readonly" class="ui-widget-content" value="${driver2_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<ul class="inputIcons">
							<li id="addDriver2" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
					</div>
					<!-- <input type="text" name="driver2_gl" class="ui-widget-content" value="${driver1_gl!}" data-scope="global" style="border: 0;width: 100%;"/> -->
				</td>
				<td class="middle" >
					<input type="text" name="certFwzg4Driver2_gl" class="ui-widget-content" value="${certFwzg4Driver2_gl!}" data-scope="global" style="border: 0;width: 80px;"/>
				</td>
				<td class="last" >
				<!-- 
					<input type="checkbox" name="isCharger4driver2" Style="width:1em;" value="${isNewCar!}" /><label>责任人</label>	
					<input type="checkbox" name="isNoCharger4driver2" Style="width:1em;" value="${isChangeCharger!}" /><label>非责任人</label>	
				 --> 	
				 	<select name="charger4driver2" class="ui-widget-content"  data-validate="required"
					        data-scope="global" style="border: none;">
					        <option></option>
							<option value="责任人">责任人</option>
							<option value="非责任人">非责任人</option>
					</select>
				</td>
			</tr>
		</table>
	</div>
</form>