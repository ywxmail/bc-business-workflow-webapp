<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carActive.inform4dispatchCarForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carActive/inform4dispatchCar.js'>
	<div class="bc-grid header" style="padding : 0px 0px;height: 100%;">
		<table cellspacing="0"  cellpadding="0" style="width: 100%;">
			<tr class="ui-state-default row">
				<td class="first" style="width: 130em;text-align: center;">车牌</td>
				<td class="middle" style="width: 130em;text-align: center;">分公司</td>
				<td class="middle" style="width: 130em;text-align: center;">司机1</td>
				<td class="last" style="text-align: center;">司机2</td>
			</tr>
			<tr>
				<!--<td class="first" >&nbsp;</td>-->
				<td class="first">
					<div style="position:relative;">
						<input type="text" name="plate" value="${plate!plate_gl!}" style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;" class="ui-widget-content" readonly="readonly" data-validate="required"/>
						<input type="hidden" name="plate_gl" class="ui-widget-content" value="${plate!plate_gl!}" data-scope="global" />
						<input type="hidden" name="carId_gl" class="ui-widget-content" value="${carId_gl!}" data-scope="global" />
						<ul class="inputIcons">
							<li id="addCar" class="inputIcon ui-icon ui-icon-circle-plus" title='选择车辆'/>
						</ul>
					</div>
			    </td>
				<td class="middle" >
					<input type="text" name="filiale" readonly="readonly" class="ui-widget-content" value="${filiale!filiale_gl!}" 
						style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
					<input type="hidden" name="filiale_gl" class="ui-widget-content" value="${filiale!filiale_gl!}" data-scope="global" />
					<input type="hidden" name="filialeId_gl" class="ui-widget-content" value="${filialeId_gl!}" data-scope="global" data-type="long"/>
				</td>
				<td class="middle" >
					<div style="position:relative;">
						<input type="text" name="driver1" readonly="readonly" class="ui-widget-content" value="${driver1!driver1_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<input type="hidden" name="driver1_gl" class="ui-widget-content" value="${driver1!driver1_gl!}" data-scope="global" />
						<input type="hidden" name="driverId1_gl" class="ui-widget-content" value="${driverId1_gl!}" data-scope="global" />
						<input type="hidden" name="certFwzg4Driver1_gl" class="ui-widget-content" value="${certFwzg4Driver1_gl!}" data-scope="global" />
						<ul class="inputIcons">
							<li id="addDriver1" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
					</div>
				</td>
				<td class="last" >
					<div style="position:relative;">
						<input type="text" name="driver2" readonly="readonly" class="ui-widget-content" value="${driver2!driver2_gl!}" 
							style="width:90%;height:100%;border:none;margin:0;padding:0 0 0 2px;background:none;"/>
						<input type="hidden" name="driver2_gl" class="ui-widget-content" value="${driver2!driver2_gl!}" data-scope="global" />
						<input type="hidden" name="driverId2_gl" class="ui-widget-content" value="${driverId2_gl!}" data-scope="global" />
						<input type="hidden" name="certFwzg4Driver2_gl" class="ui-widget-content" value="${certFwzg4Driver2_gl!}" data-scope="global" />
						<ul class="inputIcons">
							<li id="addDriver2" class="inputIcon ui-icon ui-icon-circle-plus" title='选择司机'/>
						</ul>
					</div>
				</td>
			</tr>
		</table>
	</div>
</form>