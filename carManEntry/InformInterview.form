<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carManEntry.InformInterviewForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carManEntry/InformInterview.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
	<div style="width:640px;">
		<div class="label" style="display:inline-block;">司机是否参加入职考试：</div>
		<div class="value" style="display:inline-block;">
			<input type="hidden"  name="iiIsJoin" data-scope="global" data-type="boolean">
			<input type="radio" class="ignore" name="iiIsJoin_temp" value="true" checked style="width:auto;" />
			<label>参加</label>
			<input type="radio" class="ignore" name="iiIsJoin_temp" value="false"  style="width:auto;" />
			<label>不参加</label>
		</div>
	</div>
	<#else>
	<div style="width:640px;">
		<div class="text">
			司机是否参加入职考试：<#if iiIsJoin??><#if iiIsJoin>参加<#else>不参加</#if></#if>。
		</div>
	</div>
	</#if>
</form>