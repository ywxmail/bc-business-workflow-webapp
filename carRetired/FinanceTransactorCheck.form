<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.carRetired.FinanceTransactorCheckForm"
	data-js='bc-business-workflow/carRetired/FinanceTransactorCheck.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
		<div style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">结算情况：</div>
			<textarea name="ftcDesc"  data-scope="global" rows="3" style="width:80%;" class="ui-widget-content noresize" />
		</div>
	<#else>
		<#if ftcDesc??&&ftcDesc!="">
		<div  style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">结算情况：</div>
			${ftcDesc}	
		</div>
		</#if>
	</#if>
</form>