<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carManEntry.RoadTestForm"
	data-js='js:bc_identity,bc-business/bs.js,bc-business-workflow/carManEntry/RoadTest.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
	<div style="width:640px;">
		<div class="ui-widget-content" style="border-width:1px 1px 0 0;margin-bottom:8px;">
				<div  style="position:relative;border-bottom:1px solid #CCC;padding: 0.25em;">
					<span class="text">路试</span>
				</div>
				<table  class="table bc-grid" cellspacing="0" cellpadding="0" style="width:640px;">
					<tr class="header row">
						<td class="middle" style="width: 8em;">项目</td>
						<td class="middle" style="width: 8em;">考试情况</td>
						<td class="middle" style="width: 25em;">备注</td>
						<td class="last" style="min-width: 0.001em;border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">驾驶操作</td>
						<td class="middle">
							<select name="rtRoadSenseResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtRoadSenseDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">车速</td>
						<td class="middle">
							<select name="rtSpeedResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtSpeedDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">车距</td>
						<td class="middle">
							<select name="rtSpaceResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtSpaceDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">变道</td>
						<td class="middle">
							<select name="rtChangeResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtChangeDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">定点停车</td>
						<td class="middle">
							<select name="rtStopResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtStopDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">安全意识</td>
						<td class="middle">
							<select name="rtSecurityResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtSecurityDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">交通法规</td>
						<td class="middle">
							<select name="rtTrafficRuleResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtTrafficRuleDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">道路熟识</td>
						<td class="middle">
							<select name="rtRoad2KnowWellResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtRoad2KnowWellDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">语言表达</td>
						<td class="middle">
							<select name="rtSpeakResult" class="ui-widget-content" data-scope="global" style="width:100%;height:100%;border:none;padding:0 0 0 2px;">
						        <option value="通过">通过</option>
								<option value="不通过">不通过</option>
								<option value="未参加">未参加</option>
							</select>
						</td>
						<td class="middle">
							<input type="text" name="rtSpeakDesc" class="ui-widget-content" data-scope="global"
								style="width:100%;height:100%;border:none;padding:0 0 0 2px;background:none;">
						</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
				</table>
		</div>
		<div style="padding-left: 5px;">
			<div class="label" style="display: inline-block;">考核意见：</div>
			<div class="value" style="display: inline-block;">
				<select name="rtResult" class="ui-widget-content" data-scope="global" style="width:8em;" >
			        <option value="合格">合格</option>
					<option value="还需评估">还需评估</option>
					<option value="不合格">不合格</option>
				</select>
			</div>
		</div>
	</div>
	<#else>
	<div style="width:640px;">
		<div class="ui-widget-content" style="border-width:1px 1px 0 0;margin-bottom:8px;">
				<div style="position:relative;border-bottom:1px solid #CCC;padding: 0.25em;">
					<span class="text">路试</span>
				</div>
				<table  class="table bc-grid" cellspacing="0" cellpadding="0" style="width:640px;">
					<tr class="header row">
						<td class="middle" style="width: 8em;">项目</td>
						<td class="middle" style="width: 8em;">考试情况</td>
						<td class="middle" style="width: 25em;">备注</td>
						<td class="last" style="min-width: 0.001em;border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">驾驶操作</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtRoadSenseResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtRoadSenseDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">车速</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpeedResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpeedDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">车距</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpaceResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpaceDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">变道</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtChangeResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtChangeDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">定点停车</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtStopResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtStopDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">安全意识</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSecurityResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSecurityDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">交通法规</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtTrafficRuleResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtTrafficRuleDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">道路熟识</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtRoad2KnowWellResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtRoad2KnowWellDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
					<tr class="ui-widget-content row">
						<td class="middle" style="text-align: center">语言表达</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpeakResult!}</td>
						<td class="middle" style="padding:0 0 0 2px;text-align:left;">${rtSpeakDesc!}</td>
						<td class="last" style="border-right:1px solid #CCC;"></td>
					</tr>
				</table>
		</div>
		<div style="display: inline-block;">
			<div class="text">考核意见：${rtResult!}</div>
		</div>
	</div>
	</#if>
</form>