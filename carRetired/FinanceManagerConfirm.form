<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.carRetired.FinanceManagerConfirmForm"
	data-js='bc-business-workflow/carRetired/FinanceManagerConfirm.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
		<div style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">计财部经理意见：</div>
			<textarea name="fmcDesc"  data-scope="global" rows="3" style="width:80%;" class="ui-widget-content noresize" />
		</div>
	<#else>
		<#if fmcDesc??>
		<div  style="padding-left:0.5em;">
			<div style="display:inline-block;vertical-align:top;">计财部经理意见：</div>
			${fmcDesc}	
		</div>
		</#if>
	</#if>
</form>